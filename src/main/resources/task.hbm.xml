<?xml version='1.0'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ru.yandex.startapp.HibernateScheme">

    <class name="Task" table="task">
        <id name="taskId" type="int" column="task_id" >
            <generator class="assigned" />
        </id>

        <property name="masterId" >
            <column name="master_id" />
        </property>
        <property name="caseId" >
            <column name="case_id" />
        </property>
        <property name="caseText" >
            <column name="case_text" />
        </property>
        <property name="caseStatus" >
            <column name="case_status" />
        </property>
        <property name="caseExecuteBefore" >
            <column name="case_execute_before" />
        </property>
        <property name="roomNumber" >
            <column name="room_number" />
        </property>
        <property name="userEmail" >
            <column name="user_email" />
        </property>
        <property name="userFirstName" >
            <column name="user_first_name" />
        </property>
        <property name="userLastName" >
            <column name="user_last_name" />
        </property>
    </class>

</hibernate-mapping>